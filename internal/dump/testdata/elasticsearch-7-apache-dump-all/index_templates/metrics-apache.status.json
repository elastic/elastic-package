                        },
                        "type": "long"
                      },
                      "busy": {
                        "meta": {
                          "metric_type": "gauge"
                        },
                        "type": "long"
                      }
                    }
                  },
                  "connections": {
                    "properties": {
                      "async": {
                        "properties": {
                          "closing": {
