# Gemini API key for Gemini models
# llm.gemini.api_key: "your-gemini-api-key"
# Gemini model ID (optional, defaults to gemini-3-flash-preview)
# llm.gemini.model: "gemini-3-flash-preview"

## LLM Tracing (OpenTelemetry/Phoenix)
# Enable or disable LLM tracing (defaults to true)
# llm.tracing.enabled: true
# Tracing endpoint URL (defaults to http://localhost:6006/v1/traces)
# llm.tracing.endpoint: "http://localhost:6006/v1/traces"
# Tracing API key (optional, for authenticated endpoints)
# llm.tracing.api_key: "your-tracing-api-key"
# Project name for tracing (defaults to elastic-package)
# llm.tracing.project_name: "elastic-package"

## Fleet settings
# Interval for the auto-install content packages task (Kibana 9.2+). Defaults to "10m".
# stack.fleet_auto_install_task_interval: "10m"
#
# Enable auto-install of content packages in Fleet (Kibana 8.7+). Defaults to false.
# stack.fleet_auto_install_content_packages_enabled: true
